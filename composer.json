{
    "name": "devopdan/uniji",
    "version": "1.0.0",
    "description": "A Laravel package for handling Emoji's in Unicode (UTF8/16BE/Surrogate Pairs)",
    "type": "library",
    "require": {
        "illuminate/support": "^12.0",
        "php": "^8.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DevOpDan\\Uniji\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevOpDan\\Uniji\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevOpDan\\Uniji\\UnijiServiceProvider"
            ],
            "aliases": {
                "Uniji": "DevOpDan\\Uniji\\Facades\\Uniji"
            }
        }
    },
    "authors": [
        {
            "name": "DevOpDan"
        }
    ],
    "minimum-stability": "dev"
}
